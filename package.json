{
  "name": "get-outside",
  "version": "1.0.0",
  "description": "A full stack MERN application connecting activities, places and crowd sourced information",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "seed": "node Seeds/activitiesSeed.js && node Seeds/parksSeed.js ",
    "install": "cd client && npm install",
   "build": "cd client && npm run build",
   "heroku-postbuild": "npm run build && node Seeds/activitiesSeed.js && node Seeds/parksSeed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plnorris97/get-outside.git"
  },
  "author": "Tricia Norris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plnorris97/get-outside/issues"
  },
  "homepage": "https://github.com/plnorris97/get-outside#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "install": "^0.12.2",
    "jquery": "^3.3.1",
    "mdbreact": "^4.8.7",
    "mongoimport": "^1.0.6",
    "mongojs": "^2.6.0",
    "mongoose": "^5.4.4",
    "npm": "^6.6.0-next.1",
    "react": "^16.7.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.7.0",
    "react-parallax": "^2.1.0",
    "react-router-dom": "^4.4.0-beta.6",
    "reactstrap": "^7.0.2",
    "styled-components": "^4.1.3",
    "update": "^0.7.4",
    "video-react": "^0.13.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
